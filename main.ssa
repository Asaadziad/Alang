data $fmt_int = {b "%d", b 0}
function w $isOdd(w %x){
@start
%s0 =w copy %x
%s1 =w copy 2
%s0 =w rem %s0, %s1
%le0 =w copy %s0
%s1 =w copy 0
%re0 =w copy %s1
%s2 =w ceqw %le0, %re0
%condition0 =w copy %s2
@ifstmt
jnz %condition0, @ift0, @iff0
@ift0
%s4 =w copy 0

%r =w copy %s4
jmp @retstmt
@iff0
@ifend0
%s5 =w copy 1

%r =w copy %s5
jmp @retstmt
@retstmt

ret %r
}
export function   $main(){
@start
%s6 =w copy 5
%x =w copy %s6
%s7 =w copy 9
%y =w copy %s7
%s8 =w copy 11
%z =w copy %s8
%s9 =w copy 3
%s9 =w call $isOdd(w %s9)
%condition9 =w copy %s9
@ifstmt
jnz %condition9, @ift9, @iff9
@ift9
call $printf(l $fmt_int, w %z)
jmp @ifend9
@iff9
call $printf(l $fmt_int, w %y)

@ifend9
@retstmt

ret

}
